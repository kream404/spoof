        },
        {
          "name": "indicatorid",
          "type": "range",
          "values": "1, 2, 3, 4, 5, 6"
        },
        {
          "name": "amount",
          "type": "number",
          "min": -100,
          "max": 5000
        },
        {
          "name": "updated_at",
          "type": "timestamp",
          "format": "02/01/2006"
        },

        {
          "name": "email",
          "type": "email"
        }
      ]
    }
  ]
}
